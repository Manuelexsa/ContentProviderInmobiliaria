<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kronos.contentpinmobiliaria" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <provider
            android:name="com.example.kronos.contentpinmobiliaria.Proveedor"
            android:authorities="com.example.kronos.contentpinmobiliaria.proveedor"
            android:enabled="true"
            android:exported="true" >
        </provider>
        <activity
            android:name=".Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Foto"
            android:label="@string/title_activity_foto" >
        </activity>
        <activity
            android:name=".Insert"
            android:label="@string/title_activity_insert" >
        </activity>
    </application>

</manifest>
